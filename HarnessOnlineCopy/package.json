{
  "name": "ibis-ui",
  "version": "0.0.14",
  "private": true,
  "scripts": {
    "start": "npm run delete && npm run build:prod && npm run serve:prod",
    "dev": "npm run delete && npm run build && npm run serve && npm run watch",
    "test": "npm run lint && npm run coverage",
    "build": "webpack",
    "build:prod": "webpack --env=prod",
    "coverage": "cross-env NODE_ENV=test nyc mocha --recursive --reporter=nyan tests/**/*",
    "delete": "del build",
    "lint": "eslint src",
    "lint:report": "eslint src -o ./reports/report-results.html -f html",
    "serve": "pm2 start pm2.dev.json ",
    "serve:prod": "pm2 start pm2.prod.json --no-daemon",
    "test:file": "cross-env NODE_ENV=test nyc mocha --recursive --reporter=nyan",
    "watch": "watch \"npm run build\" ./src --wait=1.5"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "body-parser": "1.17.2",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "es6-promise": "4.1.1",
    "express": "4.15.3",
    "http-status-codes": "1.1.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "pm2": "2.5.0",
    "prop-types": "15.5.10",
    "query-string": "4.3.4",
    "react": "15.6.1",
    "react-bootstrap": "0.31.1",
    "react-data-grid": "2.0.53",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-native-action-button": "2.7.2",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "redux": "3.7.1",
    "redux-thunk": "2.2.0",
    "styled-components": "2.1.1",
    "url": "0.11.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.7",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.0.2",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "cross-env": "5.0.1",
    "del-cli": "1.1.0",
    "enzyme": "2.9.1",
    "eslint": "4.1.1",
    "eslint-plugin-react": "7.1.0",
    "file-loader": "0.11.2",
    "jsdomify": "3.1.0",
    "mocha": "3.4.2",
    "nyc": "11.0.3",
    "proxyquire": "1.8.0",
    "react-test-renderer": "15.6.1",
    "supertest": "3.0.0",
    "uglify-js": "2.8.22",
    "uglifyjs-webpack-plugin": "0.4.6",
    "url-loader": "0.5.9",
    "watch": "1.0.2",
    "webpack": "3.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
